{
    "name": "vscode-k9",
    "displayName": "shakti",
    "description": "shakti(k9) language support for vscode",
    "author": {
        "name": "Jo Shinonome",
        "email": "jshinonome@gmail.com"
    },
    "version": "0.0.5",
    "homepage": "https://github.com/jshinonome/vscode-k9",
    "publisher": "jshinonome",
    "engines": {
        "vscode": "^1.52.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jshinonome/vscode-k9.git"
    },
    "categories": [
        "Programming Languages"
    ],
    "icon": "icon.png",
    "main": "./dist/client.js",
    "activationEvents": [
        "onLanguage:k"
    ],
    "contributes": {
        "languages": [
            {
                "id": "k",
                "aliases": [
                    "k9",
                    "k",
                    "shakti"
                ],
                "extensions": [
                    ".k"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "k",
                "scopeName": "source.k",
                "path": "./syntaxes/k.tmLanguage.json"
            }
        ],
        "commands": [
            {
                "command": "k-term.sendCurrentLine",
                "title": "Send Current Line to Terminal",
                "category": "shakti-k9"
            },
            {
                "command": "k-term.sendSelection",
                "title": "Send Current Selection to Terminal",
                "category": "shakti-k9"
            }
        ],
        "keybindings": [
            {
                "command": "k-term.sendCurrentLine",
                "key": "ctrl+shift+enter"
            },
            {
                "command": "k-term.sendSelection",
                "key": "ctrl+shift+e"
            }
        ]
    },
    "scripts": {
        "clean": "rm -rf dist",
        "vscode:package": "vsce package",
        "vscode:publish": "vsce publish",
        "compile": "tsc -p ./",
        "watch": "tsc --watch -p ./",
        "vscode:prepublish": "webpack --mode production",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "test-compile": "tsc -p ./"
    },
    "dependencies": {
        "path": "^0.12.7"
    },
    "devDependencies": {
        "@types/node": "^14.14.14",
        "@types/vscode": "^1.52.0",
        "ts-loader": "^8.0.12",
        "typescript": "^4.1.3",
        "vscode-test": "^1.4.1",
        "webpack": "^5.11.0",
        "webpack-cli": "^4.2.0"
    }
}
